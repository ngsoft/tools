{
    "name": "ngsoft/tools",
    "description": "A set of reusable tools used on my projects.",
    "keywords": ["arrayaccess", "session", "export", "dot", "data", "collection", "list", "sequence", "map", "set",
        "psr-14", "event", "dispatcher"],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "suggest": {
        "symfony/event-dispatcher": "Provides tools that allow your application components to communicate with each other by dispatching events and listening to them."
    },
    "provide": {
        "psr/event-dispatcher-implementation": "^1.0",
        "psr/container-implementation": "^1.0"
    },
    "require-dev": {},
    "require": {
        "php": ">=7.4",
        "symfony/polyfill-php80": "^1.20",
        "symfony/polyfill-mbstring": "^1.20",
        "psr/event-dispatcher": "^1",
        "psr/log": "^1",
        "psr/container": "^1"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
