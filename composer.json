{
    "name": "ngsoft/tools",
    "description": "A set of reusable tools used on my projects.",
    "keywords": [
        "arrayaccess", "session", "export", "collection", "list", "map", "set", "container", "di",
        "psr-11", "enum", "BackedEnum", "attribute", "reflection", "regexp", "timer", "stopwatch"
    ],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "suggest": {},
    "provide": {
        "psr/container-implementation": "^2.0"
    },
    "require-dev": {
        "fig/cache-util": "^2",
        "psr/cache": "^1|^2|^3",
        "symfony/event-dispatcher": "^6.1"
    },
    "require": {
        "php": ">=8.1",
        "symfony/polyfill-mbstring": "^1.25",
        "psr/log": "^1|^2|^3",
        "psr/container": "^2",
        "psr/event-dispatcher": "^1"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\": "src/",
            "": "lib/"
        },
        "files": ["lib/bootstrap.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
