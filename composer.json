{
    "name": "ngsoft/tools",
    "description": "A set of reusable tools used on my projects.",
    "keywords": ["phpcache", "argv", "arrayaccess", "session"],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "require-dev": {
        "psr/log": "*",
        "psr/container": "*"
    },
    "require": {
        "php": ">=7.1",
        "symfony/polyfill-mbstring": "*",
        "psr/cache": "*",
        "psr/simple-cache": "*"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\Tools\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
